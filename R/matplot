table1 <- read.table("XXXXXXXXXX.txt", header=T)
mean <- read.table("YYYYYYYYYY.txt", header=T)
matplot(table1, xlim=c(1,4), ylim=c(0,3),
	type="l", lty="solid", col = rgb(1, 0.5, 0.5),
	xlab = "Point", ylab = "Value", main = "Group") 
par(new=T)
matplot(mean, xlim=c(1,4), ylim=c(0,3), type="l", lty="solid", lwd = 3, col = 1)
